{
  "name": "drupal/dropsolid_rocketship_profile",
  "description": "The installation profile for Dropsolid products.",
  "type": "drupal-profile",
  "authors": [
    {
      "name": "Kevin Van Belle",
      "homepage": "https://www.drupal.org/u/kevinvb",
      "role": "Maintainer"
    },
    {
      "name": "Brent Gees",
      "homepage": "https://www.drupal.org/u/brentgees",
      "role": "Maintainer"
    },
    {
      "name": "Nick Vanpraet",
      "homepage": "https://www.drupal.org/u/grayle",
      "role": "Maintainer"
    }
  ],
  "require": {
    "drupal/address": "^1.12",
    "drupal/allowed_formats": "^2.0",
    "drupal/block_content_permissions": "^1.11",
    "drupal/captcha": "^1.10 || ^2.0",
    "drupal/chosen": "^3.0",
    "drupal/config_import_single": "^2.0",
    "drupal/critical_css": "^1.19",
    "drupal/devel": "^5.0",
    "drupal/devel_php": "^1.5",
    "drupal/disable_language": "^1.0",
    "drupal/dropsolid_purge": "^1.0",
    "drupal/environment_indicator": "^4.0",
    "drupal/facets": "^2.0",
    "drupal/geocoder": "^3.34 || ^4.9",
    "drupal/geofield": "^1.53",
    "drupal/geofield_map": "^3.0",
    "drupal/http_cache_control": "^2.1",
    "drupal/imageapi_optimize": "^4.0",
    "drupal/imageapi_optimize_binaries": "^1.0",
    "drupal/imageapi_optimize_resmushit": "^2.0",
    "drupal/link_attributes": "^1.14",
    "drupal/memcache": "^2.5",
    "drupal/minifyhtml": "^2.0",
    "drupal/office_hours": "^1.11",
    "drupal/path_redirect_import": "^2.0",
    "drupal/purge": "^3.4",
    "drupal/r4032login": "^2.2",
    "drupal/rabbit_hole": "^1.0",
    "drupal/redirect": "^1.8",
    "drupal/reroute_email": "^2.2",
    "drupal/rocketship_core": "^6.1.0",
    "drupal/seckit": "^2.0",
    "drupal/simple_recaptcha": "^1.0",
    "drupal/stage_file_proxy": "^2.1",
    "drupal/symfony_mailer": "^1.2",
    "drupal/twig_xdebug": "^1.3",
    "drupal/unified_twig_ext": "^1.1",
    "drupal/views_exposed_filter_blocks": "^1.3",
    "kint-php/kint": "^3.3"
  },
  "support": {
    "docs": "https://drupal.org/project/dropsolid_rocketship_profile"
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "cweagans/composer-patches": true
    }
  },
  "extra": {
    "patches": {
      "drupal/purge": {
        "Give destruct service tags priority. Depends on related core patch": "https://www.drupal.org/files/issues/2021-10-25/3008776.add-priority-to-needs_destruct.12.patch"
      }
    }
  }
}
